pod 'GooglePlaces'
pod 'GoogleMaps'

target 'Lunch' do
    # Comment this line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for Lunch
    
    target 'LunchTests' do
        inherit! :search_paths
        # Pods for testing
    end
    
    target 'LunchUITests' do
        inherit! :search_paths
        # Pods for testing
    end
    
end

